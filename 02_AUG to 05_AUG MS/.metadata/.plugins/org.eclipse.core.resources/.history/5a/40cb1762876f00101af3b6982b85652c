package com.wipro.payment_ms.entity;

@Entity
@Table(name="payment")
@Data
public class Payment {
	@Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    int id;
    
	@Column(name="payment_mode")
    String paymentMode;
	
	@Column(name="amount")
    Double amount;
	
	@Column(name="payment_description")
    String paymentDescription;
}
