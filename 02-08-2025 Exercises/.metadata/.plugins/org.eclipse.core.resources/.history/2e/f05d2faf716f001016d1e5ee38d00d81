package com.wipro.entity.relationaldemo.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.wipro.entity.relationaldemo.entity.Movie;
import com.wipro.entity.relationaldemo.repo.GenreRepo;
import com.wipro.entity.relationaldemo.repo.MovieRepo;
import com.wipro.entity.relationaldemo.service.MovieService;

@Service
public class MovieServiceImpl implements MovieService {

	@Autowired
	MovieRepo movieRepo;
	
	@Autowired
	GenreRepo genreRepo;
	
	@Override
	public void save(Movie movie) {
		MovieRepo.save(movie);

	}

	@Override
	public List<Movie> findAll() {
		// TODO Auto-generated method stub
		return movieRepo.findAll();
	}

}
