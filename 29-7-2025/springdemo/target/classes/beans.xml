 <beans xmlns="http://www.springframework.org/schema/beans"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.springframework.org/schema/beans
                               http://www.springframework.org/schema/beans/spring-beans.xsd">
  
  
  <!--  <bean id="college" class="com.wipro.springdemo.beans.College">
  		<property name="collegeName"  value="SSGMCE" />
        <property name="department"  ref="department" />
  </bean>  -->  
  
  <bean id="college" class="com.wipro.springdemo.beans.College">
  		<constructor-arg value="SSGMCE" />
  		<constructor-arg ref="department" />
  </bean>                 
                               
  <bean id="department" class="com.wipro.springdemo.beans.Department">
  		<property name="deptCode"  value="EXTC" />
        <property name="departmentName"  value="Electronics and Telecommunication" />
  </bean>
</beans>
